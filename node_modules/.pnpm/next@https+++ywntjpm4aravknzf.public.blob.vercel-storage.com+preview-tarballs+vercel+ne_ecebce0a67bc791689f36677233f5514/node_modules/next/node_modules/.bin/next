#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules/next/dist/bin/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules/next/dist/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules/next/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules/next/dist/bin/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules/next/dist/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules/next/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/next@https+++ywntjpm4aravknzf.public.blob.vercel-storage.com+preview-tarballs+vercel+ne_ecebce0a67bc791689f36677233f5514/node_modules:/Users/sebbie/throwaway/repro-chrome-transition-indicator-bug/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/next" "$@"
else
  exec node  "$basedir/../../dist/bin/next" "$@"
fi
